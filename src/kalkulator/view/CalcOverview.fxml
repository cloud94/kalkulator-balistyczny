                <CategoryAxis fx:id="xAxis" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="yAxis" label="Wysokość" side="LEFT" />
              </yAxis>
            </LineChart>
            <Button layoutX="68.0" layoutY="295.0" mnemonicParsing="false" onAction="#onSymulujClick" prefHeight="41.0" prefWidth="113.0" text="Symuluj">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </top>
</BorderPane>
