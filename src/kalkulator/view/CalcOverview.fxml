         <children>
            <TitledPane animated="false" layoutX="30.0" layoutY="55.0" text="Parametry pocisku">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <ComboBox fx:id="rodzajPocisku" layoutX="25.0" layoutY="14.0" prefWidth="150.0" />
                        <Text layoutX="15.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Masa pocisku [g]" />
                        <Text layoutX="14.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Prędkość pocz. [m/s]" />
                        <Text layoutX="14.0" layoutY="134.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wysokość pocz. [m]" />
                        <Button layoutX="41.0" layoutY="161.0" mnemonicParsing="false" text="Zapisz konfigurację" />
